5415 silly resolved     author: { name: 'Mikeal Rogers', email: 'mikeal.rogers@gmail.com' },
5415 silly resolved     repository: { type: 'git', url: 'http://github.com/mikeal/request.git' },
5415 silly resolved     bugs: { url: 'http://github.com/mikeal/request/issues' },
5415 silly resolved     engines: [ 'node >= 0.3.6' ],
